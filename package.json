    "serialize-error": "^7.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@eventicle/eventicle-utilities": "^0.0.12",
    "@microsoft/api-documenter": "^7.19.10",
    "@microsoft/api-extractor": "^7.30.0",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4.14.172",
    "@types/loglevel": "^1.5.4",
