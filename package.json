    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.2.4",
    "typescript-bundle": "^1.0.14"
